{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "darkaonline/l5-swagger": "^10.1",
        "doctrine/annotations": "^2.0",
        "laravel/framework": "^11.31",
        "laravel/sanctum": "^4.2",
        "laravel/tinker": "^2.9",
        "nwidart/laravel-modules": "^12.0",
        "spatie/laravel-permission": "^6.24",
        "stancl/tenancy": "^3.9"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.1",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Modules\\User\\": "Modules/User/app/",
            "Modules\\User\\Database\\Factories\\": "Modules/User/database/factories/",
            "Modules\\Auth\\": "Modules/Auth/app/",
            "Modules\\Auth\\Database\\Factories\\": "Modules/Auth/database/factories/",
            "Modules\\Customer\\": "Modules/Customer/app/",
            "Modules\\Customer\\Database\\Factories\\": "Modules/Customer/database/factories/",
            "Modules\\Customer\\Database\\Seeders\\": "Modules/Customer/database/seeders/",
            "Modules\\Organization\\": "Modules/Organization/app/",
            "Modules\\Organization\\Database\\Factories\\": "Modules/Organization/database/factories/",
            "Modules\\Organization\\Database\\Seeders\\": "Modules/Organization/database/seeders/",
            "Modules\\Appointment\\": "Modules/Appointment/app/",
            "Modules\\Appointment\\Database\\Factories\\": "Modules/Appointment/database/factories/",
            "Modules\\Appointment\\Database\\Seeders\\": "Modules/Appointment/database/seeders/",
            "Modules\\JobCard\\": "Modules/JobCard/app/",
            "Modules\\JobCard\\Database\\Factories\\": "Modules/JobCard/database/factories/",
            "Modules\\JobCard\\Database\\Seeders\\": "Modules/JobCard/database/seeders/",
            "Modules\\Inventory\\": "Modules/Inventory/app/",
            "Modules\\Inventory\\Database\\Factories\\": "Modules/Inventory/database/factories/",
            "Modules\\Inventory\\Database\\Seeders\\": "Modules/Inventory/database/seeders/",
            "Modules\\Invoice\\": "Modules/Invoice/app/",
            "Modules\\Invoice\\Database\\Factories\\": "Modules/Invoice/database/factories/",
            "Modules\\Invoice\\Database\\Seeders\\": "Modules/Invoice/database/seeders/",
            "Modules\\Product\\": "Modules/Product/app/",
            "Modules\\Product\\Database\\Factories\\": "Modules/Product/database/factories/",
            "Modules\\Product\\Database\\Seeders\\": "Modules/Product/database/seeders/",
            "Modules\\Pricing\\": "Modules/Pricing/app/",
            "Modules\\Pricing\\Database\\Factories\\": "Modules/Pricing/database/factories/",
            "Modules\\Pricing\\Database\\Seeders\\": "Modules/Pricing/database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Modules\\User\\Tests\\": "Modules/User/tests/",
            "Modules\\Auth\\Tests\\": "Modules/Auth/tests/",
            "Modules\\Customer\\Tests\\": "Modules/Customer/tests/",
            "Modules\\Organization\\Tests\\": "Modules/Organization/tests/",
            "Modules\\Appointment\\Tests\\": "Modules/Appointment/tests/",
            "Modules\\JobCard\\Tests\\": "Modules/JobCard/tests/",
            "Modules\\Inventory\\Tests\\": "Modules/Inventory/tests/",
            "Modules\\Invoice\\Tests\\": "Modules/Invoice/tests/",
            "Modules\\Product\\Tests\\": "Modules/Product/tests/",
            "Modules\\Pricing\\Tests\\": "Modules/Pricing/tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
