{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["resources/js/**/*", "resources/js/**/*.vue"],
  "exclude": ["node_modules", "vendor"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["resources/js/*"]
    },
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false
  }
}
