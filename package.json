{
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "dev": "vite",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint resources/js --ext .ts,.vue",
    "lint:fix": "eslint resources/js --ext .ts,.vue --fix",
    "format": "prettier --write resources/js/**/*.{ts,vue}",
    "prepare": "husky"
  },
  "dependencies": {
    "axios": "^1.7.4",
    "pinia": "^2.2.6",
    "vue": "^3.5.13",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "eslint": "^10.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-vue": "^10.8.0",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "laravel-vite-plugin": "^1.2.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.4.47",
    "prettier": "^3.8.1",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.9.3",
    "vite": "^6.0.11",
    "vue-eslint-parser": "^10.4.0",
    "vue-tsc": "^3.2.4"
  },
  "lint-staged": {
    "resources/js/**/*.{ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
